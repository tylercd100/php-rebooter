{
    "name": "tylercd100/php-rebooter",
    "description": "This package will allow you to reboot, boot, and shutdown your servers remotely.",
    "keywords": [
        "tylercd100"
    ],
    "homepage": "https://github.com/tylercd100/php-rebooter",
    "license": "MIT",
    "authors": [
        {
            "name": "Tyler Arbon",
            "email": "tylercd100@gmail.com"
        }
    ],
    "autoload":{
        "psr-4":{
            "Tylercd100\\Rebooter\\":"src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tylercd100\\Rebooter\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "guzzlehttp/guzzle": "^6.2",
        "herzult/php-ssh": "^1.1",
        "php": "^5.5.9|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.0"
    },
    "suggest": {
    }
}
